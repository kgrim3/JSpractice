function getIndexToIns(arr, num) {
  arr.sort((a,b) => a-b);
  for (let i=0; i < arr.length; i++) {
    if (num <= arr[i]) return i;
    }  
  return arr.length;
  }
  
getIndexToIns([40, 60], 50);
//return the index position of where the num should go into the given array if the array was sorted (if not already)
