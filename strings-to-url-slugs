// The global variable
var globalTitle = "Winter Is Coming";

// Only change code below this line
function urlSlug(title) {
  let temp = title.toLowerCase();
  console.log(temp);
  temp = temp.trim();
  temp = temp.split(/\s+/);
  console.log(temp);
  temp = temp.join("-");
  console.log(temp);
  return temp;
}
urlSlug(" Winter Is   Coming");
urlSlug(globalTitle);
// Only change code above this line
//take given string make all lowercase and combine them into one string with hyphens separating each word
